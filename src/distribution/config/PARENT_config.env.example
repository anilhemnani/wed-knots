# Parent config.env (one level above install dir)
# Place next to ${project.artifactId}-${project.version} folder

SPRING_PROFILES_ACTIVE=prod
JASYPT_ENCRYPTOR_PASSWORD=
DATABASE_URL=jdbc:postgresql://localhost:5432/wedknots
DATABASE_USERNAME=wedknots_user
DATABASE_PASSWORD=
PORT=8080
LOG_LEVEL=INFO
LOG_FILE=logs/wedknots.log
SPRING_CONFIG_LOCATION=file:./config/application.yml
WHATSAPP_VERIFY_TOKEN=
WHATSAPP_APP_SECRET=

# SPRING_CONFIG_LOCATION Options:
# - file:./config/application.yml              (local file, relative path)
# - file:C:/config/application.yml             (absolute path on Windows)
# - file:../config/application.yml             (parent directory)
# - classpath:application.yml                  (from JAR classpath)
# - file:./config/application-prod.yml,file:./config/overrides.yml  (multiple files)

